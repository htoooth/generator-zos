{
  "name": "generator-zos",
  "version": "1.0.4",
  "description": "zeppos-miniapp generator",
  "homepage": "",
  "type": "module",
  "author": {
    "name": "htoooth",
    "email": "ht.anglenx@gmail.com",
    "url": "https://github.com/htoooth"
  },
  "files": [
    "generators"
  ],
  "main": "generators/app/index.js",
  "keywords": [
    "yeoman-generator",
    "zeppos",
    "zepp",
    "Zepp OS"
  ],
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-xo": "^0.44.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "simple-git-hooks": "^2.10.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "@yeoman/types": "^1.2.0",
    "chalk": "^5.3.0",
    "debug": "^4.3.4",
    "lodash": "^4.17.21",
    "mkdirp": "^3.0.1",
    "radash": "^12.1.0",
    "yeoman-generator": "^7.1.1",
    "yosay": "^3.0.0"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "jest",
    "prepare": "simple-git-hooks install"
  },
  "repository": "htoooth/generator-zos",
  "license": "MIT",
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  }
}
