{
  "configVersion": "v3",
  "app": {
    "appId": <%- appId %>,
    "appName": "<%- appName %>",
    "appType": "app",
    "version": {
      "code": 1,
      "name": "0.0.1"
    },
    "icon": "icon.png",
    "vender": "Zepp OS Developer",
    "description": "A Zepp OS program"
  },
  "permissions": [
    "data:os.device.info",
    "device:os.bg_service",
    "device:os.notification"
  ],
  "runtime": {
    "apiVersion": {
      "compatible": "3.0",
      "target": "3.0",
      "minVersion": "3.0"
    }
  },
  "debug": false,
  "targets": {
    "common": {
      "module": {
        "page": {
          "pages": [
            "page/home/index.page"
          ]
        },
        "app-side": {
          "path": "app-side/index"
        },
        "setting": {
          "path": "setting/index"
        },
        "app-service": {
          "services": [
            "app-service/index"
          ]
        }
      },
      "platforms": [
        {
          "st": "r"
        },
        {
          "st": "s"
        },
        {
          "st": "b"
        }
      ],
      "designWidth": 480
    }
  },
  "i18n": {
    "en-US": {
      "appName": "<%- appName %>"
    }
  },
  "defaultLanguage": "en-US"
}
