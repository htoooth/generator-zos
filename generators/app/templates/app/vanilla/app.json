{
  "configVersion": "v3",
  "app": {
    "appId": <%- appId %>,
    "appName": "<%- appName %>",
    "appType": "app",
    "version": {
      "code": 1,
      "name": "0.0.1"
    },
    "icon": "icon.png",
    "vender": "Zepp OS Developer",
    "description": "A Zepp OS program"
  },
  "permissions": [
    "data:os.device.info"
  ],
  "runtime": {
    "apiVersion": {
      "compatible": "<%- apiLevel %>",
      "target": "<%- apiLevel %>",
      "minVersion": "<%- apiLevel %>"
    }
  },
  "targets": {
    "common": {
      "module": {
        "page": {
          "pages": [
            "page/home/index.page"
          ]
        }
      },
      "platforms": [
        {
          "st": "r",
          "dw": 480
        },
        {
          "st": "s",
          "dw": 390
        }
      ]
    }
  },
  "i18n": {
    "en-US": {
      "appName": "<%- appName %>"
    },
    "zh-CN": {
      "appName": "Zepp OS应用"
    }
  },
  "defaultLanguage": "en-US",
  "debug": false
}
