{
  "configVersion": "v3",
  "app": {
    "appId": <%- appId %>,
    "appName": "<%- appName %>-watchface",
    "appType": "watchface",
    "version": {
      "code": 1,
      "name": "0.0.1"
    },
    "icon": "images/preview.png",
    "vender": "Zepp OS Developer",
    "description": "A Zepp OS watchface application"
  },
  "permissions": [],
  "runtime": {
    "apiVersion": {
      "compatible": "<%- apiLevel %>",
      "target": "<%- apiLevel %>",
      "minVersion": "<%- apiLevel %>"
    }
  },
  "targets": {
    "common": {
      "module": {
        "watchface": {
          "path": "watchface/index",
          "main": 1,
          "editable": 1,
          "lockscreen": 1
        }
      },
      "platforms": [
        {
          "st": "r"
        }
      ],
      "designWidth": 480
    }
  },
  "i18n": {
    "en-US": {
      "appName": "<%- appName %>-Watchface"
    },
    "zh-CN": {
      "appName": "Zepp OS表盘"
    }
  },
  "defaultLanguage": "en-US",
  "debug": false
}
