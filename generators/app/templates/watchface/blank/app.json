{
  "configVersion": "v2",
  "app": {
    "appId": <%- appId %>,
    "appName": "<%- appName %>",
    "version": {
      "code": 1,
      "name": "0.0.1"
    },
    "vender": "Zepp OS Developer",
    "description": "A Zepp OS WatchFace Program",
    "icon": "images/preview.png"
  },
  "permissions": [
  ],
  "runtime": {
    "apiVersion": {
      "compatible": "1.0",
      "target": "1.0",
      "minVersion": "1.0"
    }
  },
  "targets": {
    "gtr-3-pro": {
      "module": {
        "watchface": {
          "path": "watchface/gtr-3-pro/index",
          "main": 1,
          "editable": 1,
          "lockscreen": 1
        }
      },
      "platforms": [
        {
          "name": "madrid",
          "deviceSource": 229
        },
        {
          "name": "madridw",
          "deviceSource": 230
        },
        {
          "name": "balance_1",
          "deviceSource": 8519936
        },
        {
          "name": "balance_2",
          "deviceSource": 8519937
        },
        {
          "name": "balance_3",
          "deviceSource": 8519939
        }
      ],
      "designWidth": 480
    }
  },
  "i18n": {
    "en-US": {
      "appName": "<%- appName %>"
    }
  },
  "defaultLanguage": "en-US",
  "debug": false
}
